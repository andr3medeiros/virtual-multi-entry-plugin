  "keywords": [
    "vite",
    "plugin",
    "multi-entry",
    "virtual",
    "rollup",
    "build",
    "bundler",
    "typescript"
  ],
  "author": "Andr√© Medeiros <andr3medeiros@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/virtual-multi-entry.plugin.cjs",
  "module": "./dist/virtual-multi-entry.plugin.js",
  "types": "./dist/virtual-multi-entry.plugin.d.ts",
  "exports": {
    ".": {
      "types": "./dist/virtual-multi-entry.plugin.d.ts",
      "import": "./dist/virtual-multi-entry.plugin.js",
      "require": "./dist/virtual-multi-entry.plugin.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andr3medeiros/virtual-multi-entry-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/andr3medeiros/virtual-multi-entry-plugin/issues"
  },
  "homepage": "https://github.com/andr3medeiros/virtual-multi-entry-plugin#readme",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
